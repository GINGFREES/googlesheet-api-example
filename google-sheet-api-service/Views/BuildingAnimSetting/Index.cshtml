@model IEnumerable<google_sheet_api_service.Models.BuildingAnimSetting>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.buildingSize)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.levelUpAnimName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.checkAnimValue)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.levelUpAnimValue)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.buildingSize)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.levelUpAnimName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.checkAnimValue)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.levelUpAnimValue)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
