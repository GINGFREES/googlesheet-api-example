@model IEnumerable<google_sheet_api_service.Models.BuildingLevelSize>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.buildingName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.buildingLevel)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.islandGid)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.buildingGid)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.checkAnim)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.levelUpAnim)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.buildingSize)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.buildingName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.buildingLevel)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.islandGid)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.buildingGid)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.checkAnim)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.levelUpAnim)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.buildingSize)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
